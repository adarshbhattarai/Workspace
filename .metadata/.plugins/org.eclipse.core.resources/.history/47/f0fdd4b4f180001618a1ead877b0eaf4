package Programming_Assignment_2;

import java.util.*;

public class LargestNumber {
    private static String largestNumber(String[] a) {
       
        String result = "";
        List<String> rslt = new ArrayList<>();
        for (int i = 0; i < a.length; i++) {
        	char firstchar = a[i].charAt(0);
        	
        	for(int j =i; j < a.length; j++){
        		char nextFirstchar = a[j].charAt(0);
        		if(nextFirstchar > firstchar){
        			//swap(a[i], a[j]);
        			String temp = a[i];
        			a[i] = a[j];
        			a[j] = a[i];
        			firstchar =nextFirstchar;
        		}
        		
        		else if(nextFirstchar == firstchar){
        			String suff = a[i] + a[j];
        			String pre = a[j] + a[i];
        			if(Integer.parseInt(pre) > Integer.parseInt(suff)){
        				String temp = a[i];
            			a[i] = a[j];
            			a[j] = a[i];
            			firstchar =nextFirstchar;
        			}
        		}
        		
        	}
            result += a[i];
        }
        return result;
    }

  

	public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        String[] a = new String[n];
        for (int i = 0; i < n; i++) {
            a[i] = scanner.next();
        }
        System.out.println(largestNumber(a));
    }
}

