package divide_conquer;

import java.util.Scanner;

public class BinarySearch {
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			Scanner scanner = new Scanner(System.in);
			int n = scanner.nextInt();
			int[] array = new int[n];
			for(int i=0; i<n;i++){
				array[i]= Math.random();
			}
	}

}
