package CodeFights;

public class remainingFib {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(remainFib("20162131232142343241232443389781123q8712983472429873498123"));

	}

	static String remainFib(String number) {
	    int fib1 = 0;
	    int fib2 = 1;
	    int fibn;
	    boolean found = false;
	    int k = 1;
	    int num = 0;
	    while(!found){       
	        if(k<fib2){
	            num++;
	            k++;
	        }
	        else{
	        fibn = fib1 + fib2;
	        fib1 = fib2;
	        fib2 = fibn;
	            k= fib1 + 1;
	        }
	        if(num  == Integer.parseInt(number)){
	            found = true;
	        }        
	    }
	    return String.valueOf(k-1);
	    

	}

}
